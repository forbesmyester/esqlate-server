{
  "name": "esqlate-server",
  "version": "0.0.0",
  "description": "Server for the Esqlate project",
  "main": "index.js",
  "dependencies": {
    "ajv": "^6.10.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "esqlate-queue": "^1.0.0",
    "express": "^4.17.1",
    "pg": "^7.12.1",
    "pg-native": "^3.0.0",
    "random-crypto-string": "^1.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
